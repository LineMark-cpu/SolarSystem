<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.text.Text?>

<FlowPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.SampleController">
   <children>
      <BorderPane prefHeight="200.0" prefWidth="200.0">
         <top>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Here you can retrieve data from the Solar System at Ejby Maskinfabrik" BorderPane.alignment="CENTER" />
         </top>
         <right>
            <Button mnemonicParsing="false" onAction="#changeScreenBack" text="Go back" BorderPane.alignment="CENTER" />
         </right>
         <left>
            <Button mnemonicParsing="false" onAction="#retrieveDateWithMaxEnergy" text="Date with maximum Energy" BorderPane.alignment="CENTER" />
         </left>
         <center>
            <Button mnemonicParsing="false" onAction="#retrieveMaxEnergy" text="Maximum Energy" BorderPane.alignment="CENTER" />
         </center>
      </BorderPane>
   </children>
</FlowPane>
